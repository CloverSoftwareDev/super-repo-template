{
  "name": "super-repo-template",
  "version": "1.0.0",
  "description": "Template of a ai monorepo in TS",
  "workspaces": [
    "packages/*"
  ],
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "format": "npx @biomejs/biome format --write",
    "lint": "npx @biomejs/biome lint --write",
    "check": "npx @biomejs/biome check --write",
    "cli": "tsx packages/cli/src/index.ts",
    "build": "npm run cli -- build",
    "build:deno": "bash scripts/build-deno.sh",
    "start": "npm run start -w packages/api"
  },
  "keywords": [
    "monorepo"
  ],
  "author": "CloverSoftwareDev",
  "license": "MIT",
  "devDependencies": {
    "@biomejs/biome": "2.3.13"
  }
}
